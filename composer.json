{
    "name": "johnkrovitch/deploy-bundle",
    "type": "symfony-bundle",
    "authors": [
        {
            "name": "johnkrovitch",
            "email": "arnaudfrezet@gmail.com"
        }
    ],
    "require": {
        "php": "^7.1",
        "symfony/console": "^4.1",
        "phpunit/phpunit": "^7.4",
        "symfony/dependency-injection": "^4.1",
        "symfony/http-kernel": "^4.1",
        "symfony/config": "^4.1",
        "symfony/yaml": "^4.1",
        "johnkrovitch/configuration": "^1.3",
        "symfony/finder": "^4.1",
        "twig/twig": "^2.5"
    },
    "autoload": {
        "psr-4": {
            "JK\\DeployBundle\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "JK\\DeployBundle\\Tests\\": "tests/DeployBundle/"
        }
    },
    "config": {
        "bin-dir": "bin"
    },
    "require-dev": {
        "symfony/debug": "^4.1",
        "symfony/var-dumper": "^4.1",
        "friendsofphp/php-cs-fixer": "^2.13"
    }
}
