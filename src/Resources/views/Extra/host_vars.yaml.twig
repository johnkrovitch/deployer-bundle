---
### Ansible ###
ansible_host: {{ ansible_host }}
ansible_user: {{ ansible_user }}
ansible_port: {{ ansible_port }}

{% for moduleName, module in data %}
### {{ moduleName | capitalize }} ###
{{ moduleName }}:
{% for name, value in module %}
    {% if value is iterable %}{{ name }}: {{ value | join(' ') }}{% else %}{{ name }}: {{ value }}{% endif %}

{% endfor %}

{% endfor %}
